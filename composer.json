{
    "name": "newelement/dmp-plex",
    "description": "Plex plugin for DMP",
    "license": "GPL-2.0-or-later",
    "homepage": "http://github.com/newelement/dmp-plex",
    "authors": [
        {
            "name": "Don Jones",
            "email": "newelementdesigns@me.com"
        }
    ],
    "require": {
        "newelement/digital-movie-poster": "*"
    },
    "autoload": {
        "psr-4": {
            "Newelement\\DmpPlex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Newelement\\DmpPlex\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Newelement\\DmpPlex\\DmpPlexServiceProvider"
            ]
        }
    }
}
